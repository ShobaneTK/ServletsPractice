<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AddServlet</servlet-name>
        <servlet-class>za.ac.tut.web.AddServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NumFemaleServlet</servlet-name>
        <servlet-class>za.ac.tut.web.NumFemaleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NumMaleServlet</servlet-name>
        <servlet-class>za.ac.tut.web.NumMaleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>YoungestServlet</servlet-name>
        <servlet-class>za.ac.tut.web.YoungestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AverageServlet</servlet-name>
        <servlet-class>za.ac.tut.web.AverageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LongestServlet</servlet-name>
        <servlet-class>za.ac.tut.web.LongestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddServlet</servlet-name>
        <url-pattern>/AddServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NumFemaleServlet</servlet-name>
        <url-pattern>/NumFemaleServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NumMaleServlet</servlet-name>
        <url-pattern>/NumMaleServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>YoungestServlet</servlet-name>
        <url-pattern>/YoungestServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AverageServlet</servlet-name>
        <url-pattern>/AverageServlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LongestServlet</servlet-name>
        <url-pattern>/LongestServlet.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
